[
  {
    "key": "q",
    "command": "oil-code.close",
    "when": "editorLangId == oil"
  },
  {
    "key": "ctrl+]",
    "command": "workbench.action.terminal.focusNext",
    "when": "terminalFocus"
  },
  {
    "key": "shift+k",
    "command": "editor.action.showHover",
    "when": "editorTextFocus && vim.mode != 'Insert'"
  },
  {
    "key": "ctrl+[",
    "command": "workbench.action.terminal.focusPrevious",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+m",
    "command": "workbench.action.toggleMaximizedPanel",
    "when": "terminalFocus && panelFocus"
  },
  {
    "key": "ctrl+m",
    "command": "workbench.action.toggleMaximizeEditorGroup",
    "when": "editorPartMaximizedEditorGroup || editorPartMultipleEditorGroups"
  },
  {
    "key": "ctrl+shift+b",
    "command": "workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "ctrl+k",
    "command": "selectPrevSuggestion",
    "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.navigateUp",
    "when": "vim.mode != 'Visual' && vim.mode != 'VisualLine' && vim.mode != 'VisualBlock' && vim.mode != 'Insert'"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.navigateDown",
    "when": "vim.mode != 'Visual' && vim.mode != 'VisualLine' && vim.mode != 'VisualBlock' && vim.mode != 'Insert'"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.quickOpenSelectNext",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.quickOpenSelectPrevious",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.navigateLeft"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.navigateRight"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.previousPanelView",
    "when": "panelFocus"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.files.action.focusOpenEditorsView",
    "when": "sideBarFocus && explorerViewletVisible"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.files.action.focusFilesExplorer",
    "when": "sideBarFocus && explorerViewletVisible && !filesExplorerFocus"
  },
  {
    "key": "shift+cmd+pageup",
    "command": "workbench.action.increaseViewSize",
    "when": "editorFocus || panelFocus"
  },
  {
    "key": "shift+cmd+pagedown",
    "command": "workbench.action.decreaseViewSize",
    "when": "editorFocus || panelFocus"
  },
  {
    "key": "ctrl+i",
    "command": "workbench.action.terminal.moveToEditor"
  },
  {
    "key": "ctrl+shift+\\",
    "command": "editor.emmet.action.matchTag"
  },
  {
    "key": "ctrl+shift+w",
    "command": "editor.emmet.action.wrapWithAbbreviation"
  },
  {
    "key": "ctrl+shift+t",
    "command": "editor.emmet.action.removeTag"
  },
  {
    "key": "h",
    "command": "editor.action.scrollLeftHover",
    "when": "editorHoverFocused"
  },
  {
    "key": "j",
    "command": "editor.action.scrollDownHover",
    "when": "editorHoverFocused"
  },
  {
    "key": "k",
    "command": "editor.action.scrollUpHover",
    "when": "editorHoverFocused"
  },
  {
    "key": "l",
    "command": "editor.action.scrollRightHover",
    "when": "editorHoverFocused"
  },
  {
    "key": "q",
    "command": "workbench.action.closeSidebar",
    "when": "sideBarVisible && sideBarFocus && !inputFocus"
  },
  {
    "key": "ctrl+i",
    "command": "extension.vim_ctrl+i",
    "when": "editorTextFocus && vim.active && vim.use<C-i> && !inDebugRepl"
  },
  {
    "key": "ctrl+q",
    "command": "workbench.action.togglePanel",
    "when": "editorTextFocus && panelVisible"
  },
  {
    "key": "ctrl+q",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "editorTextFocus && !panelVisible"
  },
  {
    "key": "ctrl+q",
    "command": "workbench.action.togglePanel",
    "when": "terminalFocus"
  },
  {
    "command": "workbench.action.toggleSidebarVisibility",
    "key": "ctrl+e"
  },
  {
    "command": "workbench.files.action.focusFilesExplorer",
    "key": "ctrl+e",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+n",
    "command": "explorer.newFile",
    "when": "explorerViewletFocus"
  },
  {
    "key": "ctrl+f",
    "command": "explorer.newFolder",
    "when": "explorerViewletFocus"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.terminal.clear",
    "when": "terminalFocus && terminalHasBeenCreated && !accessibilityModeEnabled || terminalFocus && terminalProcessSupported && !accessibilityModeEnabled"
  },
  {
    "key": "cmd+f",
    "command": "editor.action.startFindReplaceAction",
    "when": "editorFocus || editorIsOpen"
  },
  {
    "key": "shift+cmd+f",
    "command": "workbench.action.replaceInFiles"
  },
  {
    "key": "alt+cmd+p",
    "command": "composer.openAddContextMenu",
    "when": "composerFocused && !editorTextFocus"
  },
  {
    "key": "ctrl+p",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "ctrl+shift+p",
    "command": "projectManager.listProjects"
  },
  {
    "key": "ctrl+shift+o",
    "command": "projectManager.listProjectsNewWindow"
  },
  {
    "key": "cmd+n",
    "command": "explorer.newFile",
    "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  },
  {
    "key": "cmd+shift+n",
    "command": "explorer.newFolder",
    "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  },
  {
    "key": "cmd+backspace",
    "command": "deleteFile",
    "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  },
  {
    "key": "ctrl+shift+l",
    "command": "editor.action.smartSelect.expand",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+h",
    "command": "editor.action.smartSelect.shrink",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+n",
    "command": "workbench.action.showCommands"
  },
  {
    "key": "ctrl+w",
    "command": "windsurf.prioritized.chat.open"
  },
  {
    "key": "cmd+l",
    "command": "-windsurf.prioritized.chat.open"
  },
  {
    "key": "ctrl+f",
    "command": "workbench.action.findInFiles",
    "when": "!explorerViewletFocus && !searchViewletVisible"
  },
  {
    "key": "ctrl+f",
    "command": "workbench.action.toggleAuxiliaryBar",
    "when": "searchViewletVisible"
  },
  {
    "key": "ctrl+j",
    "command": "search.focus.nextInputBox",
    "when": "searchViewletVisible && searchViewletFocus && inputFocus"
  },
  {
    "key": "ctrl+k",
    "command": "search.focus.previousInputBox",
    "when": "searchViewletVisible && searchViewletFocus && inputFocus"
  },
  {
    "key": "ctrl+k",
    "command": "search.action.focusSearchFromResults",
    "when": "searchViewletVisible && hasSearchResult && !inputFocus"
  },
  {
    "key": "ctrl+j",
    "command": "list.focusDown",
    "when": "searchViewletVisible && searchResultsFocus"
  },
  {
    "key": "ctrl+k",
    "command": "list.focusUp",
    "when": "searchViewletVisible && searchResultsFocus"
  },
  {
    "key": "shift+enter",
    "command": "workbench.action.terminal.sendSequence",
    "args": {
      "text": "\\\r\n"
    },
    "when": "terminalFocus"
  },
  {
    "key": "cmd+r cmd+m",
    "command": "-workbench.action.toggleMaximizeEditorGroup",
    "when": "editorPartMaximizedEditorGroup || editorPartMultipleEditorGroups"
  }
]
