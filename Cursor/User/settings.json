{
  "editor.fontSize": 17,
  "editor.lineHeight": 20,
  "editor.formatOnSave": true,
  "editor.autoClosingDelete": "always",
  "editor.tabSize": 1,
  "window.zoomLevel": -0.2,
  "workbench.editor.showTabs": "none",
  "debug.javascript.codelens.npmScripts": "never",
  "editor.accessibilitySupport": "off",
  "editor.linkedEditing": true,
  "editor.cursorBlinking": "solid",
  "files.trimTrailingWhitespace": true,
  "editor.fontFamily": "Fira Code",
  "editor.fontWeight": "450",
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "editor.fontLigatures": true,
  "workbench.editor.enablePreview": false,
  "editor.lineNumbers": "relative",
  "redhat.telemetry.enabled": false,
  "editor.guides.bracketPairs": "active",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "terminal.integrated.cwd": "${fileDirname}",
  "editor.minimap.enabled": false,
  "todo-tree.regex.regex": "(//)\\s*($TAGS).*(\\n(?=\\s*//\\s{1,}\\S).*?)*",
  "todo-tree.highlights.defaultHighlight": {
    "type": "line"
  },
  "todo-tree.highlights.customHighlight": {
    "BUG": {
      "icon": "bug",
      "foreground": "#F92672",
      "iconColour": "#F92672",
      "gutterIcon": true
    },
    "HACK": {
      "icon": "flame",
      "foreground": "#FD971F",
      "iconColour": "#FD971F",
      "gutterIcon": true
    },
    "FIXME": {
      "icon": "tools",
      "foreground": "#AE81FF",
      "iconColour": "#AE81FF",
      "gutterIcon": true
    },
    "TODO": {
      "icon": "check",
      "foreground": "#A6E22E",
      "iconColour": "#A6E22E",
      "gutterIcon": true
    },
    "NOTE": {
      "icon": "note",
      "foreground": "#E6DB74",
      "iconColour": "#E6DB74",
      "gutterIcon": true
    },
    "Q": {
      "icon": "question",
      "foreground": "#66D9EF",
      "iconColour": "#66D9EF",
      "gutterIcon": true
    }
  },
  "todo-tree.general.tags": ["BUG", "HACK", "FIXME", "TODO", "NOTE", "Q"],
  "breadcrumbs.enabled": false,
  "vim.leader": "<Space>",
  "vim.useSystemClipboard": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "rgba(250, 165, 0, 0.7)",
  "vim.highlightedyank.duration": 100,
  "workbench.activityBar.orientation": "vertical",
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "editor.cursorSurroundingLines": 10,
  "vim.searchHighlightColor": "red",
  "vim.insertModeKeyBindings": [],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["<S-h>"],
      "after": ["^"]
    },
    {
      "before": ["<leader>", "c", "r"],
      "commands": ["editor.action.changeAll"]
    },
    {
      "before": ["<leader>", "g", "g"],
      "commands": ["lazygit-vscode.toggle"]
    },
    {
      "before": ["<S-l>"],
      "after": ["g", "_"]
    },
    {
      "before": ["]", "t"],
      "commands": ["todo-tree.goToNext"],
      "silent": true
    },
    {
      "before": ["[", "t"],
      "commands": ["todo-tree.goToPrevious"],
      "silent": true
    },
    {
      "before": ["]", "h"],
      "commands": ["editor.action.dirtydiff.previous"],
      "silent": true
    },
    {
      "before": ["[", "h"],
      "commands": ["editor.action.dirtydiff.next"],
      "silent": true
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.nextInFiles"],
      "silent": true
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prevInFiles"],
      "silent": true
    },
    {
      "before": ["-"],
      "commands": ["vsnetrw.open"],
      "when": "editorLangId != vsnetrw"
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"],
      "silent": true
    },
    {
      "before": ["leader", "|"],
      "commands": [":vsplit"]
    },
    {
      "before": ["leader", "-"],
      "commands": [":split"]
    },
    {
      "before": ["leader", "m"],
      "commands": ["workbench.action.toggleMaximizeEditorGroup"]
    },
    {
      "before": ["<C-u>"],
      "after": ["2", "5", "k"]
    },
    {
      "before": ["<C-d>"],
      "after": ["2", "5", "j"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["leader", "w"],
      "commands": [":w!"]
    },
    {
      "before": ["leader", "/"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": ["leader", "Q"],
      "commands": ["workbench.action.closeEditorsInGroup"]
    },
    {
      "before": ["leader", "q"],
      "commands": [":q!"]
    },
    {
      "before": ["leader", "space"],
      "commands": ["workbench.action.quickOpen"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<C-j>"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["<C-k>"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    {
      "before": ["leader", "/"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": ["<S-h>"],
      "after": ["^"]
    },
    {
      "before": ["<S-l>"],
      "after": ["g", "_"]
    }
  ],
  "vim.operatorPendingModeKeyBindingsNonRecursive": [
    {
      "before": ["<S-h>"],
      "after": ["^"]
    },
    {
      "before": ["<S-l>"],
      "after": ["g", "_"]
    }
  ],
  "errorLens.fontSize": "13",
  "errorLens.gutterIconsEnabled": "true",
  "errorLens.messageEnabled": false,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.defaultProfile.osx": "fish",
  "liveServer.settings.donotShowInfoMsg": true,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "launch": {
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Program",
        "program": "${file}"
      }
    ],
    "compounds": []
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "javascript.suggest.paths": false,
  "typescript.suggest.paths": false,
  "workbench.sideBar.location": "right",
  "explorer.openEditors.visible": 3,
  "cursor.composer.textSizeScale": 1.3,
  "terminal.external.osxExec": "Ghostty.app",
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "workbench.colorTheme": "Kanagawa Black"
}
