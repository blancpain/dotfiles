{
	"name": "flash-vscode",
	"displayName": "flash.vscode",
	"description": "Minimal port of the flash.nvim plugin, bringing fast, label-based code navigation to Visual Studio Code",
	"version": "0.0.9",
	"engines": {
		"vscode": "^1.96.0"
	},
